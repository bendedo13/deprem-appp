{
  "expo": {
    "name": "QuakeSense: Life-Saving Tracker",
    "slug": "quakesense",
    "version": "1.0.0",
    "description": "QuakeSense is the ultimate life-saving technology designed to track global seismic activities with unprecedented speed and reliability.",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "extra": {
      "apiUrl": "http://46.4.123.77:8001"
    },
    "plugins": [
      "expo-sensors",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Konum, deprem sinyali göndermek için kullanılır."
        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-1173265458993328~5191916494"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "minSdkVersion": 23,
            "kotlinVersion": "1.9.0"
          }
        }
      ]
    ],
    "android": {
      "package": "com.quakesense",
      "versionCode": 1,
      "permissions": [
        "android.permission.VIBRATE",
        "android.permission.USE_FULL_SCREEN_INTENT",
        "android.permission.INTERNET",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION"
      ],
      "googleServicesFile": "./google-services.json"
    },
    "ios": {
      "bundleIdentifier": "com.quakesense"
    }
  }
}